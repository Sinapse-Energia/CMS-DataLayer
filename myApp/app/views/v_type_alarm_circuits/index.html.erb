<p id="notice"><%= notice %></p>

<h1>Listing V Type Alarm Circuits</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_type_alarm_circuits.each do |v_type_alarm_circuit| %>
      <tr>
        <td><%= v_type_alarm_circuit.name %></td>
        <td><%= link_to 'Show', v_type_alarm_circuit %></td>
        <td><%= link_to 'Edit', edit_v_type_alarm_circuit_path(v_type_alarm_circuit) %></td>
        <td><%= link_to 'Destroy', v_type_alarm_circuit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

