<p id="notice"><%= notice %></p>

<h1>Listing Vf Lampmonitor Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vf_lampmonitor_events.each do |vf_lampmonitor_event| %>
      <tr>
        <td><%= vf_lampmonitor_event.name %></td>
        <td><%= link_to 'Show', vf_lampmonitor_event %></td>
        <td><%= link_to 'Edit', edit_vf_lampmonitor_event_path(vf_lampmonitor_event) %></td>
        <td><%= link_to 'Destroy', vf_lampmonitor_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

