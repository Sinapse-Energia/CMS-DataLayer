<p id="notice"><%= notice %></p>

<h1>Listing V Occurrences</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_occurrences.each do |v_occurrence| %>
      <tr>
        <td><%= v_occurrence.name %></td>
        <td><%= link_to 'Show', v_occurrence %></td>
        <td><%= link_to 'Edit', edit_v_occurrence_path(v_occurrence) %></td>
        <td><%= link_to 'Destroy', v_occurrence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V occurrence', new_v_occurrence_path %>
