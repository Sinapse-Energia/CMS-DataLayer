<p id="notice"><%= notice %></p>

<h1>Listing F Functions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @f_functions.each do |f_function| %>
      <tr>
        <td><%= f_function.name %></td>
        <td><%= link_to 'Show', f_function %></td>
        <td><%= link_to 'Edit', edit_f_function_path(f_function) %></td>
        <td><%= link_to 'Destroy', f_function, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New F function', new_f_function_path %>
