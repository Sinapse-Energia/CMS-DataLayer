<p id="notice"><%= notice %></p>

<h1>Listing Adesc Fbridges</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>V storagetype</th>
      <th>Unit</th>
      <th>Minvalue</th>
      <th>Maxvalue</th>
      <th>Regex</th>
      <th>Enumeration</th>
      <th>Read only</th>
      <th>Maxinstances</th>
      <th>Descr</th>
      <th>Lightcommand</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adesc_fbridges.each do |adesc_fbridge| %>
      <tr>
        <td><%= adesc_fbridge.name %></td>
        <td><%= adesc_fbridge.v_storagetype %></td>
        <td><%= adesc_fbridge.unit %></td>
        <td><%= adesc_fbridge.minvalue %></td>
        <td><%= adesc_fbridge.maxvalue %></td>
        <td><%= adesc_fbridge.regex %></td>
        <td><%= adesc_fbridge.enumeration %></td>
        <td><%= adesc_fbridge.read_only %></td>
        <td><%= adesc_fbridge.maxinstances %></td>
        <td><%= adesc_fbridge.descr %></td>
        <td><%= adesc_fbridge.lightcommand %></td>
        <td><%= link_to 'Show', adesc_fbridge %></td>
        <td><%= link_to 'Edit', edit_adesc_fbridge_path(adesc_fbridge) %></td>
        <td><%= link_to 'Destroy', adesc_fbridge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

