<p id="notice"><%= notice %></p>

<h1>Listing V Typecommands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Short name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_typecommands.each do |v_typecommand| %>
      <tr>
        <td><%= v_typecommand.name %></td>
        <td><%= v_typecommand.short_name %></td>
        <td><%= link_to 'Show', v_typecommand %></td>
        <td><%= link_to 'Edit', edit_v_typecommand_path(v_typecommand) %></td>
        <td><%= link_to 'Destroy', v_typecommand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V typecommand', new_v_typecommand_path %>
