<p id="notice"><%= notice %></p>

<h1>Listing V Dynamicoperations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_dynamicoperations.each do |v_dynamicoperation| %>
      <tr>
        <td><%= v_dynamicoperation.name %></td>
        <td><%= link_to 'Show', v_dynamicoperation %></td>
        <td><%= link_to 'Edit', edit_v_dynamicoperation_path(v_dynamicoperation) %></td>
        <td><%= link_to 'Destroy', v_dynamicoperation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V dynamicoperation', new_v_dynamicoperation_path %>
