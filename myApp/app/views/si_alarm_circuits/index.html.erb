<p id="notice"><%= notice %></p>

<h1>Listing Si Alarm Circuits</h1>

<table>
  <thead>
    <tr>
      <th>Threshold</th>
      <th>Num repeat</th>
      <th>Notif email</th>
      <th>Ids lista email</th>
      <th>Notif sms</th>
      <th>Ids list sms</th>
      <th>V type alarm circuit</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @si_alarm_circuits.each do |si_alarm_circuit| %>
      <tr>
        <td><%= si_alarm_circuit.threshold %></td>
        <td><%= si_alarm_circuit.num_repeat %></td>
        <td><%= si_alarm_circuit.notif_email %></td>
        <td><%= si_alarm_circuit.ids_lista_email %></td>
        <td><%= si_alarm_circuit.notif_sms %></td>
        <td><%= si_alarm_circuit.ids_list_sms %></td>
        <td><%= si_alarm_circuit.v_type_alarm_circuit %></td>
        <td><%= si_alarm_circuit.name %></td>
        <td><%= link_to 'Show', si_alarm_circuit %></td>
        <td><%= link_to 'Edit', edit_si_alarm_circuit_path(si_alarm_circuit) %></td>
        <td><%= link_to 'Destroy', si_alarm_circuit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

