<p id="notice"><%= notice %></p>

<h1>Listing V Storagetypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_storagetypes.each do |v_storagetype| %>
      <tr>
        <td><%= v_storagetype.name %></td>
        <td><%= link_to 'Show', v_storagetype %></td>
        <td><%= link_to 'Edit', edit_v_storagetype_path(v_storagetype) %></td>
        <td><%= link_to 'Destroy', v_storagetype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V storagetype', new_v_storagetype_path %>
