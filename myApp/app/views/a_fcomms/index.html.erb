<p id="notice"><%= notice %></p>

<h1>Listing A Fcomms</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Seqbridge</th>
      <th>Seqcms</th>
      <th>Adesc fcomm</th>
      <th>F communication</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @a_fcomms.each do |a_fcomm| %>
      <tr>
        <td><%= a_fcomm.value %></td>
        <td><%= a_fcomm.seqbridge %></td>
        <td><%= a_fcomm.seqcms %></td>
        <td><%= a_fcomm.adesc_fcomm %></td>
        <td><%= a_fcomm.f_communication %></td>
        <td><%= link_to 'Show', a_fcomm %></td>
        <td><%= link_to 'Edit', edit_a_fcomm_path(a_fcomm) %></td>
        <td><%= link_to 'Destroy', a_fcomm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

