<p id="notice"><%= notice %></p>

<h1>Listing Si General Informations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Broker</th>
      <th>Port</th>
      <th>User</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @si_general_informations.each do |si_general_information| %>
      <tr>
        <td><%= si_general_information.name %></td>
        <td><%= si_general_information.broker %></td>
        <td><%= si_general_information.port %></td>
        <td><%= si_general_information.user %></td>
        <td><%= si_general_information.password %></td>
        <td><%= link_to 'Show', si_general_information %></td>
        <td><%= link_to 'Edit', edit_si_general_information_path(si_general_information) %></td>
        <td><%= link_to 'Destroy', si_general_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Si general information', new_si_general_information_path %>
