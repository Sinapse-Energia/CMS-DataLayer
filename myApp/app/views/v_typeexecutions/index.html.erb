<p id="notice"><%= notice %></p>

<h1>Listing V Typeexecutions</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_typeexecutions.each do |v_typeexecution| %>
      <tr>
        <td><%= v_typeexecution.description %></td>
        <td><%= link_to 'Show', v_typeexecution %></td>
        <td><%= link_to 'Edit', edit_v_typeexecution_path(v_typeexecution) %></td>
        <td><%= link_to 'Destroy', v_typeexecution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

