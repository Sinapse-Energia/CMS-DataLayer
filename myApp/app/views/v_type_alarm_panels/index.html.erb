<p id="notice"><%= notice %></p>

<h1>Listing V Type Alarm Panels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Magnitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_type_alarm_panels.each do |v_type_alarm_panel| %>
      <tr>
        <td><%= v_type_alarm_panel.name %></td>
        <td><%= v_type_alarm_panel.magnitude %></td>
        <td><%= link_to 'Show', v_type_alarm_panel %></td>
        <td><%= link_to 'Edit', edit_v_type_alarm_panel_path(v_type_alarm_panel) %></td>
        <td><%= link_to 'Destroy', v_type_alarm_panel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V type alarm panel', new_v_type_alarm_panel_path %>
